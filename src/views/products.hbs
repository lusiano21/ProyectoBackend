<h1>Productos</h1>
<section>
<article class="d-flex justify-content-around flex-wrap">
{{#each payload}}
  <div class="card" style="width:18rem;">
    <img src="{{this.image}}" class="card-img-top" alt="restaurante">
    <div class="card-body d-flex flex-column justify-content-end">
      <h5 class="card-title">{{this.name}}</h5>
      {{#each products}}
      <p class="card-text">{{this.id}}</p>
      <p class="card-text">{{this.menu}}</p>
      <p class="card-text">{{this.price}}</p>
      {{/each}}
    </div>
  </div>
{{/each}}
</article>
</section>
<p>PÃ¡gina actual: {{page}}</p>
{{#if prevLink}}
  <a href="{{prevLink}}" class="btn btn-outline-primary" type="button">Anterior</a>
{{/if}}
{{#if nextLink}}
  <a href="{{nextLink}}" class="btn btn-outline-primary" type="button">Siguente</a>
{{/if}}